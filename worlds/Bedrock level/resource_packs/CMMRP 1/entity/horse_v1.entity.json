{
  "format_version": "1.10.0",
  "minecraft:client_entity": {
    "description": {
      "identifier": "minecraft:horse",
      "materials": {
        "default": "cmm.horse_leather_armor"
      },
      "textures": {
        "base_brown": "textures/cmm/horse/horse_brown",
        "base_white": "textures/cmm/horse/horse_white",
        "base_creamy": "textures/cmm/horse/horse_creamy",
        "base_chestnut": "textures/cmm/horse/horse_chestnut",
        "base_black": "textures/cmm/horse/horse_black",
        "base_gray": "textures/cmm/horse/horse_gray",
        "base_darkbrown": "textures/cmm/horse/horse_darkbrown",
        "markings_none": "textures/cmm/horse/horse_markings_none",
        "markings_white": "textures/cmm/horse/horse_markings_white",
        "markings_whitefield": "textures/cmm/horse/horse_markings_whitefield",
        "markings_whitedots": "textures/cmm/horse/horse_markings_whitedots",
        "markings_blackdots": "textures/cmm/horse/horse_markings_blackdots",
        "armor_none": "textures/cmm/horse/armor/horse_armor_none",
        "armor_leather": "textures/cmm/horse/armor/horse_armor_leather",
        "armor_iron": "textures/cmm/horse/armor/horse_armor_iron",
        "armor_gold": "textures/cmm/horse/armor/horse_armor_gold",
        "armor_diamond": "textures/cmm/horse/armor/horse_armor_diamond"
      },
      "geometry": {
        "default": "geometry.cmm.horse.v1.and.v2"
      },
      "animations": {
        "setup": "animation.cmm.horse.v1.and.v2.setup",
        "baby_transform": "animation.cmm.horse.v1.and.v2.baby_transform",
        "walk": "animation.cmm.horse.v1.and.v2.walk",
        "eat": "animation.cmm.horse.v1.and.v2.eat",
        "stand": "animation.cmm.horse.v1.and.v2.stand",
        "tail": "animation.cmm.horse.v1.and.v2.tail"
      },
      "scripts": {
        "initialize": [
          "variable.eat_anim = 0.0;",
          "variable.mouth_anim = 0.0;"
        ],
        "pre_animation": [
          "variable.tcos0 = (Math.cos(query.modified_distance_moved * 38.17) * query.modified_move_speed / variable.gliding_speed_value) * 57.3;",
          "variable.armor_texture_slot = query.armor_texture_slot(1);",
          "variable.eat_anim = math.clamp(variable.eat_anim + (query.is_grazing ? ((1.0 - variable.eat_anim) * 0.4 + 0.05) : -variable.eat_anim * 0.4 - 0.05) * query.delta_time * 20.0, 0.0, 1.0);",
          "variable.mouth_anim = math.clamp(variable.mouth_anim + (variable.open_mouth ? ((1.0 - variable.mouth_anim) * 0.7 + 0.05) : -variable.mouth_anim * 0.7 - 0.05) * query.delta_time * 20.0, 0.0, 1.0);",
          "variable.head_x_rot = query.target_x_rotation + (query.modified_move_speed > 0.2 ? (math.cos(query.modified_distance_moved * 11.46) * 11.46 * query.modified_move_speed + query.modified_move_speed * 11.46) : 0.0);",
          "variable.inverse_max_stand_eat = (1.0 - math.max(variable.stand_anim, variable.eat_anim));",
          "variable.base_head_angle = variable.inverse_max_stand_eat * (30.0 + variable.head_x_rot + variable.mouth_anim  * math.sin((query.life_time * 20.0 + query.frame_alpha) * 57.3) * 2.86);",
          "variable.leg_stand_factor = math.cos((query.modified_distance_moved * 38.38) + 180.0);",
          "variable.leg_x_rot_anim = variable.leg_stand_factor * 45.8 * query.modified_move_speed;",
          "variable.leg1a_aabb = query.bone_aabb('leg1a');",
          "variable.y_adjust = query.is_baby ? 0.0 : (variable.leg1a_aabb.max.y - variable.leg1a_aabb.min.y) * (1.0 - (query.model_scale * 16.0));"
        ],
        "animate": [
          "setup",
          { "baby_transform": "query.is_baby" },
          "walk",
          "stand",
          { "eat": "!query.is_standing" },
          { "tail": "variable.shake_tail" }
        ]
      },
      "render_controllers": [ "controller.render.cmm.horse.v1.and.v2" ],
      "spawn_egg": {
        "texture": "spawn_egg",
        "texture_index": 23
      }
    }
  }
}
{
  "format_version": "1.8.0",
  "animations": {
    "animation.cmm.squid.look_at_target.default": {
      "loop": true,
      "bones": {
        "Head": {
          "rotation": [ "query.target_x_rotation", "query.target_y_rotation", 0 ]
        },
        "HBack": {
          "rotation": [ "-query.target_x_rotation", 0, 0 ]
        },
        "Neck": {
          "rotation": [ 0, "query.target_y_rotation * 0.5", 0 ]
        },
        "Neck1": {
          "rotation": [ 0, "query.target_y_rotation * 0.5", 0 ]
        }
      }
    },
    "animation.cmm.squid.look_at_target.gliding": {
      "loop": true,
      "bones": {
        "Head": {
          "rotation": [ -45, "query.target_y_rotation", 0 ]
        },
        "HBack": {
          "rotation": [ 45, 0, 0 ]
        },
        "Neck": {
          "rotation": [ 0, "query.target_y_rotation * 0.5", 0 ]
        },
        "Neck1": {
          "rotation": [ 0, "query.target_y_rotation * 0.5", 0 ]
        }
      }
    },
    "animation.cmm.squid.look_at_target.swimming": {
      "loop": true,
      "bones": {
        "Head": {
          "rotation": [ "math.lerp(query.target_x_rotation, -45.0, variable.swim_amount)", "query.target_y_rotation", 0 ]
        },
        "HBack": {
          "rotation": [ "-math.lerp(query.target_x_rotation, -45.0, variable.swim_amount)", 0, 0 ]
        },
        "Neck": {
          "rotation": [ 0, "query.target_y_rotation * 0.5", 0 ]
        },
        "Neck1": {
          "rotation": [ 0, "query.target_y_rotation * 0.5", 0 ]
        }
      }
    },
    "animation.cmm.squid.move": {
      "anim_time_update": "query.modified_distance_moved",
      "loop": true,
      "bones": {
        "root": {
          "position": [ 0, -8, 0 ]
        },
        "RightArm": {
          "rotation": [ 0, 0, "variable.squid.tentacle_angle - this" ]
        },
        "LeftArm": {
          "rotation": [ 0, 0, "-variable.squid.tentacle_angle - this" ]
        },
        "RightLeg": {
          "rotation": [ 0, 0, "variable.squid.tentacle_angle - this" ]
        },
        "LeftLeg": {
          "rotation": [ 0, 0, "-variable.squid.tentacle_angle - this" ]
        }
      }
    },
    "animation.cmm.squid.rotate": {
      "anim_time_update": "query.modified_distance_moved",
      "loop": true,
      "bones": {
        "root": {
          "rotation": [
            { "y": "180.0 - this" },
            { "x": "query.body_x_rotation" },
            { "y": "variable.squid.swim_rotation" }
          ]
        }
      }
    }
  }
}

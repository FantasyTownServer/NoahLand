{
	"format_version": "1.8.0",
	"animations": {
		"animation.cmm.horse.v3.baby_transform": {
			"loop": true,
			"bones": {
				"Head": {
					"scale": "1.0 + 0.5 * (1.0 - (query.model_scale * 16.0))"
				},
				"LegBL": {
					"scale": [1, "1.0 + ((0.0625 - query.model_scale) * 16.0)", 1]
				},
				"LegBR": {
					"scale": [1, "1.0 + ((0.0625 - query.model_scale) * 16.0)", 1]
				},
				"LegFL": {
					"scale": [1, "1.0 + ((0.0625 - query.model_scale) * 16.0)", 1]
				},
				"LegFR": {
					"scale": [1, "1.0 + ((0.0625 - query.model_scale) * 16.0)", 1]
				},
				"BodyBase": {
					"position": [0, "11 * ((0.0625 - query.model_scale) * 16.0) ", 0]
				},
				"Bust": {
					"scale": ["1.0 - 0.875 * (1.0 - (query.model_scale * 16.0))", 1, "1.0 - 2.0 * (1.0 - (query.model_scale * 16.0))"]
				},
				"LoinCloth": {
					"scale": [1, "1.0 + ((0.0625 - query.model_scale) * 16.0)", 1]
				}
			}
		},
		"animation.cmm.horse.v3.eat": {
			"loop": true,
			"bones": {
				"Head": {
					"rotation": ["variable.eat_anim * (math.sin((query.life_time * 20.0) * 57.3) * 5.72)", 0, 0]
				},
				"BHair": {
					"rotation": ["variable.eat_anim * (math.sin((query.life_time * 20.0) * 57.3) * -5.72)", 0, 0]
				}
			}
		},
		"animation.cmm.horse.v3.look_at_player": {
			"loop": true,
			"bones": {
				"Head": {
					"rotation": ["query.target_x_rotation", "query.target_y_rotation", 0]
				},
				"Neck": {
					"rotation": [0, "query.target_y_rotation * 0.5", 0]
				},
				"BHair": {
					"rotation": ["-query.target_x_rotation", 0, 0]
				},
				"RightArm": {
					"rotation": [0, 0, "(math.cos(query.life_time * 103.2) * 2.865) + 2.865"]
				},
				"LeftArm": {
					"rotation": [0, 0, "((math.cos(query.life_time * 103.2) * 2.865) + 2.865) *-1.0"]
				}
			}
		},
		"animation.cmm.horse.v3.rear": {
			"loop": true,
			"bones": {
				"LegBL": {
					"rotation": ["45.0 * variable.stand_anim", 0, 0]
				},
				"LegBR": {
					"rotation": ["45.0 * variable.stand_anim", 0, 0]
				},
				"LegFL": {
					"rotation": ["(-60.0 + math.cos((query.life_time * 20.0) * 34.38) * 57.3) * variable.stand_anim", 0, 0]
				},
				"LegFR": {
					"rotation": ["(-60.0 - math.cos((query.life_time * 20.0) * 34.38) * 57.3) * variable.stand_anim", 0, 0]
				},
				"Neck": {
					"rotation": ["(15.0 + variable.head_x_rot) * variable.stand_anim", 0, 0]
				},
				"Tail": {
					"rotation": ["55.0 * variable.stand_anim", 0, 0]
				},
				"Head": {
					"rotation": ["-30.0 * variable.stand_anim", 0, 0]
				},
				"BHair": {
					"rotation": ["75.0 * variable.stand_anim", 0, 0]
				},
				"RightArm": {
					"rotation": ["30.0 * variable.stand_anim", 0, "20.0 * variable.stand_anim"]
				},
				"LeftArm": {
					"rotation": ["30 * variable.stand_anim", 0, "-20 * variable.stand_anim"]
				},
				"BodyBase": {
					"rotation": ["-45.0 * variable.stand_anim", 0, 0]
				}
			}
		},
		"animation.cmm.horse.v3.tail": {
			"loop": true,
			"bones": {
				"Tail": {
					"rotation": [0, "math.cos(query.life_time * 802.0) * 57.3", 0]
				}
			}
		},
		"animation.cmm.horse.v3.walk": {
			"loop": true,
			"bones": {
				"BagL": {
					"rotation": ["variable.leg_x_rot_anim / 5.0", 0, 0]
				},
				"BagR": {
					"rotation": ["-variable.leg_x_rot_anim / 5.0", 0, 0]
				},
				"RightArm": {
					"rotation": ["-variable.tcos0", 0, 0]
				},
				"LeftArm": {
					"rotation": ["variable.tcos0", 0, 0]
				},
				"LegBL": {
					"rotation": ["(-variable.leg_stand_factor * 28.6 * query.modified_move_speed) * (1.0 - variable.stand_anim)", 0, 0]
				},
				"LegBR": {
					"rotation": ["(variable.leg_stand_factor * 28.6 * query.modified_move_speed) * (1.0 - variable.stand_anim)", 0, 0]
				},
				"LegFL": {
					"rotation": ["variable.leg_x_rot_anim * (1.0 - variable.stand_anim)", 0, 0]
				},
				"LegFR": {
					"rotation": ["-variable.leg_x_rot_anim * (1.0 - variable.stand_anim)", 0, 0]
				}
			}
		}
	}
}